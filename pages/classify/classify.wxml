<search></search>
<view class="classify">
  <scroll-view scroll-y class="classify-left">
    <block wx:for="{{ classify }}" wx:key="cat_id">
    <!-- 三元表达式 -->
      <view class="classify-left-item {{ index === activeIndex ? 'active':''}}">{{item.cat_name}}</view>
    </block>
  </scroll-view>
  <scroll-view scroll-y class="classify-right">
    <view class="classify-right-title">电视</view>
    <view class="classify-right-list">

      <block wx:for="{{ 33 }}" wx:key="">
      <view class="classify-right-item">
        <image class="classify-right-item-image"></image>
        <view class="classify-right-item-text">曲面电视</view>
      </view>
      </block>

    </view>
  </scroll-view>
</view>
